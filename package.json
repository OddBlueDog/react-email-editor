{
  "name": "@oddbluedog/react-email-editor",
  "version": "1.5.0",
  "description": "Unlayer's Email Editor Component for React.js. Changed file extensions to .jsx to work with vite. Supports react 17.",
  "main": "src/index.jsx",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
