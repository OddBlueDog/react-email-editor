  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlayer/react-email-editor.git"
  },
  "keywords": [
    "react-component"
  ],
  "bugs": {
    "url": "https://github.com/unlayer/react-email-editor/issues"
  },
